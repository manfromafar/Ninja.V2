#current-shares-chart {
	position:		absolute;
	width:			500px;
	height:			500px;
	right:			0px;
	top:			0px;
	border:			2px dotted red;	
}

#current-shares-table {
	position:		absolute;
	width:			500px;
	height:			auto;
	right:			0px;
	top:			500px;
	bottom:			0px;
	overflow-y:		scroll;
	font-size:		smaller;
}

#current-block-info {
	position:		absolute;
	width:			200px;
	height:			200px;
	left:			0px;
	top:			0px;
}

#timer {
	position:		absolute;
	width:			200px;
	height:			400px;
	top:			200px;
	left:			0px;
	border:			2px dotted blue;
}